<body>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
    <a class="navbar-brand rounded" href="#"><img
        src="https://cdn.discordapp.com/icons/737094472478883981/82fd49d1b9817f08c3dec91bb0d3dfe7.webp?size=128"
        width="30" height="30" class="d-inline-block align-top mr-2" alt="" loading="lazy">KSS</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
      aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item <% if (title == "Index") { %> active <% } %>">
          <a class="nav-link" href="/">Home <% if (title == "Index") { %><span class="sr-only">
              (current)
            </span><% } %></a>
        </li>
        <li class="nav-item <% if (title == "Sorties") { %> active <% } %>">
          <a class="nav-link" href="/sorties">Sorties <% if (title == "Sorties") { %><span class="sr-only">
              (current)
            </span><% } %></a>
        </li>
        <li class="nav-item <% if (title == "Team") { %> active <% } %>">
          <a class="nav-link" href="/team">La Team <% if (title == "Team") { %><span class="sr-only">
              (current)
            </span><% } %></a>
        </li>
        <li class="nav-item <% if (title == "Planning") { %> active <% } %>">
          <a class="nav-link" href="/planning">Le Planning <% if (title == "Planning") { %><span class="sr-only">
              (current)
            </span><% } %></a>

        </li>
      </ul>
      <ul class="navbar-nav ml-md-auto">
        <% if (userData != undefined) { %>
        <li class="nav-item <% if (title == "Se deconnecter") { %> active <% } %>">
          <a class="nav-link" href="/logOut">Se deconnecter <% if (title == "Se deconnecter") { %><span class="sr-only">
              (current)
            </span><% } %></a>
        </li>
        <% if (userData.level == 0) { %>
        <li class="nav-item <% if (title == "Admin Pannel") { %> active <% } %>">
          <a class="btn btn-info" href="/pm">Admin <% if (title == "Admin Pannel") { %><span class="sr-only">
              (current)
            </span><% } %></a>

        </li>
        <% } %>
        <% } else { %>
        <li class="nav-item <% if (title == "S'inscrire") { %> active <% } %>">
          <a class="nav-link" href="/register">S'inscrire <% if (title == "S'inscrire") { %><span class="sr-only">
              (current)
            </span><% } %></a>
        </li>
        <li class="nav-item <% if (title == "Se connecter") { %> active <% } %>">
          <a class="nav-link" href="/logIn">Se connecter <% if (title == "Se connecter") { %><span class="sr-only">
              (current)
            </span><% } %></a>
        </li>
        <% } %>

      </ul>
